<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView xmlns:fx="http://javafx.com/fxml" fx:id="possibleTable" editable="true" fixedCellSize="28" >
   <styleClass>
      <String fx:value="remove-vertical-scrollbar" />
      <String fx:value="remove-horizontal-scrollbar" />
   </styleClass>
   <placeholder>
      <Label minHeight="0" />
   </placeholder>
   <columns>
      <TableColumn fx:id="possibleNameColumn" minWidth="150" prefWidth="250" editable="false" />
      <TableColumn fx:id="possibleDescColumn" text="Beschreibung" minWidth="100" prefWidth="150" sortable="false" />
      <TableColumn fx:id="possibleVariantColumn" text="Variante" minWidth="100" prefWidth="150" resizable="false" sortable="false" />
      <TableColumn fx:id="possibleValueColumn" text="Wert" minWidth="55" maxWidth="55" resizable="false" sortable="false" style="-fx-alignment: CENTER;" />
      <TableColumn fx:id="possibleCostColumn" text="Kosten" minWidth="60" maxWidth="60" resizable="false" sortable="false" style="-fx-alignment: CENTER;" />
      <TableColumn fx:id="possibleValidColumn" minWidth="0" maxWidth="0" />
      <TableColumn fx:id="possibleSuggestedColumn" minWidth="0" maxWidth="0" />
   </columns>
</TableView>